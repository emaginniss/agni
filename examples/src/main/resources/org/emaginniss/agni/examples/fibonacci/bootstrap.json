{
    "logFile": "src/main/resources/org/emaginniss/agni/examples/fibonacci/log4j.properties",
    "subscribers": [
        "org.emaginniss.agni.examples.fibonacci.FibonacciHandler"
    ],
    "rest": {
        "connectors": {
            "http": {
                "port": 8080
            }
        },
        "endpoints": [
            {
                "path": "fib/${index}",
                "method": "GET",
                "payload": "new",
                "payloadType": "org.emaginniss.agni.examples.fibonacci.FibonacciRequest",
                "inject": {
                    "index": "${index}"
                },
                "response": "payload"
            }
        ]
    },
    "scheduler": {
    }
}